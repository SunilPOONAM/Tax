                </div>
            </div>
            <div class="col-xl-3 col-lg-4  col-md-4 col-sm-4  align-self-center py-1">

                @using (Html.BeginForm("ChangeLanguage", "Home", FormMethod.Post, new { @id = "changeLanguageForm" }))
                {
                    @Html.HiddenFor(m => m.CustomerCode)
                    @Html.HiddenFor(m => m.Language, new { @id = "languageCode" })
                }

                <select class="form-control float-right text-black" name="FooBarDropDown" id="FooBarDropDown" onchange="ChangeLang(this.value);">
                    @{
                        foreach (var i in TaxService.Models.LanguageModel.AvailableLanguages)
                        {
                            <option value="@i.LanguageCultureName" class="text-black">
