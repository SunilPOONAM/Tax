              fileData.append('chunks', base64);

              $.ajax({
                  url: appUrlSettings.Url,
                  type: "POST",
                  contentType: false,
                  processData: false,
                  data: fileData,
                  success: function (response) {
                      if (response == "success") {
                          alert("Form submitted successfully !");
