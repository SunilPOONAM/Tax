        function uploadFiles(event) {
            const file = event.target.files[0]
            const filereader = new FileReader()
            filereader.onloadend = function (evt) {
                if (evt.target.readyState === FileReader.DONE) {
                    const uint = new Uint8Array(evt.target.result)
                    let bytes = []
                    uint.forEach((byte) => {
                        bytes.push(byte.toString(16))
                    })
                    const hex = bytes.join('').toUpperCase()
                    uploads = getMimetype(hex);
                    if (uploads == "Unknown filetype" || file.type == "application/x-zip-compressed") {
                        uploads = null;
                        document.getElementById('FileUpload1').value = "";
                        document.getElementById('FileUpload2').value = "";
                        alert('Only valid document, image and pdf files are allowed to upload');
                    } else {
                        if (file.size >= 10485760) {
                            uploads = [];
                            document.getElementById('FileUpload1').value = "";
                            document.getElementById('FileUpload2').value = "";
                            alert("File too big, please select a file less than 10 MB");
                        } else if (file.size < 1024) {
                            uploads = [];
                            document.getElementById('FileUpload1').value = "";
                            document.getElementById('FileUpload2').value = "";
                            alert("File too small, please select a file greater than 1 KB");
                        } else {
                            $('#fileDetails').removeClass('d-none').addClass('d-block');
                            $('#nameList').append("<li>" + file.name + "</li>");
                            fileData.append(file.name, file);
                        }
                    }
                }
            }

            const blob = file.slice(0, 4);
            filereader.readAsArrayBuffer(blob);
        }



      const getMimetype = (signature) => {
        switch (signature) {
          case '89504E47':
            return 'image/png'
          case '47494638':
            return 'image/gif'
          case '25504446':
            return 'application/pdf'
          case 'FFD8FFDB':
          case 'FFD8FFE0':
          case 'FFD8FFE1':
            return 'image/jpeg'
          case '504B34':
            return 'application/vnd.openxmlformats-officedocument.wordprocessingml.document'
          case '75706461':
            return 'text/plain';
          default:
            return 'Unknown filetype'
        }
      }

        function sendFile(file) {

